        <servlet-name>aafilter</servlet-name>
        <servlet-class>com.bwf.jia.AAfilter</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>aafilter</servlet-name>
        <servlet-pattern>/aa/bb</servlet-pattern>
    </servlet-mapping>
    <listener/>
</web-app>
